Host github.com
    User git
    Hostname ssh.github.com
    Port 443
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/daydayup

Host 192.168.88.*
    Port 22
    User root
    IdentityFile ~/.ssh/local_vm

Host *
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    LogLevel ERROR
